{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/tonconnect-manifest.json",
      "destination": "/tonconnect-manifest.json"
    }
  ],
  "headers": [
    {
      "source": "/tonconnect-manifest.json",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
